{
  "name": "remote-wsl-recommender",
  "displayName": "%displayName%",
  "version": "0.0.1",
  "private": "false",
  "description": "%description%",
  "categories": [
    "Other"
  ],
  "keywords": [
    "remote",
    "wsl",
    "remote development"
  ],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "webpack --mode none && yarn lint",
    "lint": "eslint src --ext ts",
    "patch-dev": "yarn compile && node ./build/patch-local.js .vscode-oss-dev",
    "patch-insiders": "yarn compile && node ./build/patch-local.js .vscode-insiders",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts",
    "preversion": "yarn compile",
    "postversion": "git push && git push --tags",
    "vscode:prepublish": "webpack --mode production",
    "watch": "webpack --mode none --watch"
  },
  "contributes": {
    "commands": [
      {
        "command": "remote-wsl-recommender.connectToWSL",
        "title": "%connectToWSLCommand%",
        "category": "WSL"
      },
      {
        "command": "remote-wsl-recommender.gettingStarted",
        "title": "%gettingStartedCommand%",
        "category": "WSL"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "remote-wsl-recommender.connectToWSL",
          "when": "remote-wsl-recommender.connectToWSL.available"
        },
        {
          "command": "remote-wsl-recommender.gettingStarted",
          "when": "false"
        }
      ],
      "statusBar/remoteIndicator": [
        {
          "command": "remote-wsl-recommender.connectToWSL",
          "when": "remote-wsl-recommender.connectToWSL.available",
          "group": "remote_10_wsl_0_local@2"
        },
        {
          "command": "remote-wsl-recommender.gettingStarted",
          "when": "remote-wsl-recommender.gettingStarted.available",
          "group": "remote_10_wsl_0_local@9"
        }
      ]
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "devDependencies": {
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/node": "20.x",
    "@types/vscode": "1.83.1",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "@vscode/extension-telemetry": "0.8.5",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.52.0",
    "ts-loader": "9.5.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1",
    "vscode-nls": "5.2.0",
    "vscode-nls-dev": "4.0.4",
    "vscode-tas-client": "0.1.63",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "engines": {
    "vscode": "1.73.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "icon": "resources/remote-wsl.png",
  "aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  }
}
