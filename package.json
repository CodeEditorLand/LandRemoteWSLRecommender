{
	"name": "remote-wsl-recommender",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./dist/extension.js",
	"scripts": {
		"compile": "webpack --mode none && yarn lint",
		"patch-dev": "yarn compile && node ./build/patch-local.js .vscode-oss-dev",
		"patch-insiders": "yarn compile && node ./build/patch-local.js .vscode-insiders",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"preversion": "yarn compile",
		"postversion": "git push && git push --tags",
		"vscode:prepublish": "webpack --mode production",
		"watch": "webpack --mode none --watch"
	},
	"contributes": {
		"commands": [
			{
				"category": "WSL",
				"command": "remote-wsl-recommender.connectToWSL",
				"title": "%connectToWSLCommand%"
			},
			{
				"category": "WSL",
				"command": "remote-wsl-recommender.gettingStarted",
				"title": "%gettingStartedCommand%"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "remote-wsl-recommender.connectToWSL",
					"when": "remote-wsl-recommender.connectToWSL.available"
				},
				{
					"command": "remote-wsl-recommender.gettingStarted",
					"when": "false"
				}
			],
			"statusBar/remoteIndicator": [
				{
					"command": "remote-wsl-recommender.connectToWSL",
					"group": "remote_10_wsl_0_local@2",
					"when": "remote-wsl-recommender.connectToWSL.available"
				},
				{
					"command": "remote-wsl-recommender.gettingStarted",
					"group": "remote_10_wsl_0_local@9",
					"when": "remote-wsl-recommender.gettingStarted.available"
				}
			]
		}
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"devDependencies": {
		"@types/copy-webpack-plugin": "^10.1.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.73.0",
		"@vscode/extension-telemetry": "0.7.5",
		"copy-webpack-plugin": "^11.0.0",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.5",
		"typescript-esbuild": "latest",
		"vscode-nls": "^5.2.0",
		"vscode-nls-dev": "^4.0.3",
		"vscode-tas-client": "^0.1.63",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "resources/remote-wsl.png",
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	}
}
